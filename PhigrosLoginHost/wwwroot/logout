		<script>
			// 解决 iOS 12以及更低低版本 iframe 内打开兼容性问题
			try {
				delete window.indexedDB;
			} catch (e) {}

			//读取cookies
			function getCookie(name) {
				var arr,
					reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");

				arr = document.cookie.match(reg);

				if (arr) {
					return unescape(arr[2]);
				} else {
					return null;
				}
			}

			function clientApi(apiName, params) {
				if (typeof params === "object") {
					params = JSON.stringify(params);
				}
				try {
					if (window.urlResource) {
						return window.urlResource.TapTapAPI(apiName, params);
					}

					if (window.TapTapAPI && window.TapTapAPI[apiName]) {
						return window.TapTapAPI[apiName](params);
					} else {
						return (
							window.TapTapAPI &&
							window.TapTapAPI(apiName, params)
						);
					}
				} catch (e) {
					console.error(e);
				}
			}

			function getUrlParam(paraName) {
				var url = document.location.toString();
				var arrObj = url.split("?");
				if (arrObj.length > 1) {
					var arrPara = arrObj[1].split("&");
					var arr;
					for (var i = 0; i < arrPara.length; i++) {
						arr = arrPara[i].split("=");
						if (arr != null && arr[0] == paraName) {
							return arr[1];
						}
					}
					return "";
				} else {
					return "";
				}
			}
			function getMatchMediaFn() {
				if (
					typeof window === "undefined" ||
					typeof window.matchMedia !== "function"
				) {
					return function (media) {
						return {
							media: media,
							matches: false,
						};
					};
				}
				return window.matchMedia;
			}
			function getTheme() {
				return "dark";
				// 0、优先用js bridge 取客户端的主题
				if (clientApi("tapEnv")) {
					var tapEnv = JSON.parse(clientApi("tapEnv"));
					if (tapEnv) {
						return tapEnv.THEME;
					}
				}

				// 1、兼容 query 方式取主题
				if (getUrlParam("theme_mode")) {
					return getUrlParam("theme_mode");
				}

				// 2、取cookie里的语言参数
				if (getCookie("tap_theme")) {
					return getCookie("tap_theme");
				}
				var _mediaQuery = getMatchMediaFn();
				if (
					_mediaQuery("(prefers-color-scheme: dark)") &&
					_mediaQuery("(prefers-color-scheme: dark)").matches
				) {
					return "dark";
				}
				return "light";
			}
			var isIntl = false;
			if (window.__tapEnv__) {
				if (
					window.__tapEnv__.ACCOUNT_CLUSTER_REGION === "hk" ||
					window.__tapEnv__.ACCOUNT_CLUSTER_REGION === "intl" ||
					window.__tapEnv__.ACCOUNT_IS_GLOBAL === "true"
				) {
					isIntl = true;
				}
			}

			var styleNode = document.createElement("style");
			if (getTheme() === "night" || getTheme() === "dark") {
				styleNode.innerHTML = ":root{--primary-white: #191B26}";
			} else {
				styleNode.innerHTML = ":root{--primary-white: #FFFFFF}";
			}
			if (isIntl) {
				styleNode.innerHTML = ":root{--primary-white: #000}";
				var favicon =
					document.querySelector("link[rel*='icon']") ||
					document.createElement("link");
				favicon.href =
					"/cupid-apps/web-app-account/favicon.intl.2.ico";
			}
			document.head.appendChild(styleNode);
			if (!isIntl) {
				var loadingDom = document.querySelector(".base-frame-loading");
				if (loadingDom && loadingDom.classList) {
					loadingDom.classList.remove("hide");
				}
			}

			// 设置背景色
		</script>
